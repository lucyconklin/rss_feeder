<h1>RSS Feeder</h1>

<%= form_for @audio do |f| %>
  <%= f.label :url %>:
  <%= f.url_field :url %>

  <%= f.label :date %>:
  <%= f.text_field :date %>

  <%= f.submit 'Submit' %>
<% end %>

<section class="audio-list">
  <% @audios.each do |audio| %>
    <div class="audio-element">
      <h4><%= audio.title %></h4>
      <p><%= audio.date %></p>
    </div>
  <% end %>
</section>
